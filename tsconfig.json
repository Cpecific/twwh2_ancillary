{
	"compilerOptions": {
		"target": "ES6",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"lib": [
			"ES2019"
		],
		"esModuleInterop": true,
		"downlevelIteration": true,
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@input/*": [
				"./input/*"
			]
		},
		"typeRoots": [
			"./node_modules/@types"
		],
		// "types": [
		// 	"./module.d.ts"
		// ],
	},
	// "files": [
	// 	"./module.d.ts"
	// ],
	"include": [
		"./src/**/*.ts"
	]
}